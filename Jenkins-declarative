pipeline {
   agent any

environment{
PATH="/opt/maven/maven3/bin:$PATH"
}
   stages {
      stage('Git Checkout') {
         steps {
            git credentialsId: 'GitHub', url: 'https://github.com/gangaprasadp/hello-world'
         }
      }
       stage('Maven build') {
         steps {
            sh "mvn clean package"
         }
      }
   }
}
